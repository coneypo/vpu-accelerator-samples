
{
    "rtsp":
    {
        "rtsp_server":
        [
            {
                "enable":1,
                "element":"4k_enc",
                "caps":"video/x-h264,stream-format=byte-stream,alignment=au",
                "fps":30,
                "mount_path":"/test0"
            },
            {
                "enable":1,
                "element":"1080p_enc",
                "caps":"video/x-h264,stream-format=byte-stream,alignment=au",
                "fps":30,
                "mount_path":"/test1"
            }
        ],
        "rtsp_server_port": 1121 //1-65535
    },
    "cvsdk_detection":
    {
        "enable_motion_detection":1,
        "enable_face_detection":1
    },
    "4k_mix":
    {
        "subscribe_message" : ["faces", "motions"],
        "delay_queue": "4k_queue",
        "logo":
        [
            {
                "filename":"intel.rgba",
                "width":200,
                "height":132,
                "x":16,
                "y":16
            }
        ],
        "osd":
        [
            {
                "num":10,
                "x":800,
                "y":700
            },
            {
                "x":1500,
                "y":0,
                "name":"clock",
                "font":"Sans 30"
            },
            {
                "x":600,
                "y":400,
                "name":"cpu",
                "font":"Sans 32",
                "max-stride":10
            },
            {
                "x":1000,
                "y":400,
                "name":"gpu",
                "font":"Sans 32",
                "max-stride":10
            }
        ]
    },
    "1080p_mix":
    {
        "subscribe_message" : ["faces", "motions"],
        "delay_queue": "1080p_queue",
        "logo":
        [
            {
                "filename":"intel.rgba",
                "width":200,
                "height":132,
                "x":16,
                "y":16
            }
        ],
        "osd":
        [
            {
                "num":10,
                "x":800,
                "y":700
            },
            {
                "x":1500,
                "y":0,
                "name":"clock",
                "font":"Sans 30"
            },
            {
                "x":600,
                "y":400,
                "name":"cpu",
                "font":"Sans 32",
                "max-stride":10
            },
            {
                "x":1000,
                "y":400,
                "name":"gpu",
                "font":"Sans 32",
                "max-stride":10
            }
        ]
    },
    "element":
    [
            //---------src_scale---------
            {
                "name":"src_scale_caps",
                "caps":"video/x-raw,format=NV12,width=1920,height=1080"
            },

            //---------videorate----------
            {
                "name":"videorate",
                "skip-to-first":1
            },
            {
                "name":"videorate_caps",
                "caps":"video/x-raw,framerate=30/1"
            },
            //---------4k_scale---------
            {
                "name":"4k_scale_caps",
                "caps":"video/x-raw,format=NV12,width=3840,height=2160"
            },
            //---------4k_mix---------
            {
                "name":"4k_mix_caps",
                "caps":"video/x-raw(memory:MFXSurface),format=NV12,width=3840,height=2160"
            },
            //---------4k_enc---------
            {
                "name":"4k_enc",
                //"bitrate":8192
            },
            {
                "name":"4k_enc_caps",
                "caps":"video/x-h264,stream-format=byte-stream,alignment=au",
            },
            //---------4k_encrytion---------
            {
                "name":"4k_encryption",
                 "mode":"enc"
            },
            //---------4k_encrytion---------
            {
                "name":"4k_sink",
                "location":"4k_file"
            },

            //---------1080p_scale---------
            {
                "name":"1080p_scale_caps",
                "caps":"video/x-raw,format=NV12,width=1920,height=1080"
            },
            //---------1080p_mix---------
            {
                "name":"1080p_mix_caps",
                "caps":"video/x-raw(memory:MFXSurface),format=NV12,width=1920,height=1080"
            },
            //---------1080p_enc---------
            {
                "name":"1080p_enc",
                "bitrate":4096
            },
            {
                "name":"1080p_enc_caps",
                "caps":"video/x-h264,stream-format=byte-stream,alignment=au",
            },
            //---------720p_scale---------
            {
                "name":"720p_scale_caps",
                "caps":"video/x-raw,format=NV12,width=1280,height=720"
            },
            //---------720p_mix---------
            {
                "name":"720p_mix_caps",
                "caps":"video/x-raw(memory:MFXSurface),format=NV12,width=1280,height=720"
            },
            //---------720p_enc---------
            {
                "name":"720p_enc",
                "bitrate":1536
            },
            {
                "name":"720p_enc_caps",
                "caps":"video/x-h264,stream-format=byte-stream,alignment=au",
            },

            //----------------local display-------------
            {
                "name":"scaler_display_caps",
                "caps":"video/x-raw,format=xBGR,width=1920,height=1080"
            }
    ]
}

